{
  "name": "blog2.x-mux-club",
  "version": "0.0.2",
  "description": "a blog version 2.x about dvajs and reactjs...",
  "private": true,
  "scripts": {
    "start": "cross-env ESLINT=none roadhog dev",
    "build": "cross-env ESLINT=none roadhog build",
    "build:analyze": "cross-env ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "precommit": "npm run lint-staged",
    "public": "node script/deploy.js --env=${V}"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "antd": "^3.4.0",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "dva": "^2.1.0",
    "dva-loading": "^1.0.4",
    "enquire-js": "^0.2.1",
    "moment": "^2.19.1",
    "omit.js": "^1.0.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.0",
    "react": "^16.3.1",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.1",
    "react-sublime-video": "^0.2.5",
    "url-polyfill": "^1.0.10"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.1.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.0.1",
    "lint-staged": "^6.0.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "rc-banner-anim": "^1.0.1",
    "rc-queue-anim": "^1.4.1",
    "rc-scroll-anim": "^2.3.3",
    "rc-tween-one": "^1.7.3",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.1",
    "roadhog": "^2.3.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.0.0"
  },
  "optionalDependencies": {
    "puppeteer": "^1.1.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,scss}": [
      "prettier --wirter",
      "git add"
    ],
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.scss": "stylelint --syntax scss"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
